-- Project Euler: Even Fibonacci Numbers
-- https://projecteuler.net/problem=2

let main (_): Unit -> Unit =
    let res: Int = 0;
    let i: Int = 1;
    let j: Int = 2;
    while j < 4000000 do (
        if j % 2 == 0 then set res = res + j; -- add even fibonacci number to result
        let tmp: Int = i;
        set i = j;
        set j = tmp + j
    );
    print(res) -- 4613732
