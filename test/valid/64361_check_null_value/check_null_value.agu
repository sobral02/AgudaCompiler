let testNullAssignment(x) : Int -> Unit =
    if x == unit then
        print("x is null")
    else
        print("x is not null")

let _ : Unit =
    testNullAssignment(null); -- Should print "x is null"
    testNullAssignment(1)     -- Should print "x is not null"