let compareLength (str1, str2) : (String, String) -> Int =
    let len1 : Int = length(str1);
    let len2 : Int = length(str2);
    if len1 < len2 then -1 else (
        if len1 > len2 then 1 else 0
    )

let _ : Unit =
    let string1 : String = "testee";
    let string2 : String = "teste";

    print(compareLength(string1, string2))