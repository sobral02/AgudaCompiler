-- Function to calculate the average of five numbers
let average (a, b, c, d, e) : (Int, Int, Int, Int, Int) -> Int =
  (a + b + c + d + e) / 5

let result (_) : Unit -> Int = average(10, 20, 30, 40, 50)

let main(x) : Unit -> Unit =
    print(result(x))  -- Expected result: 30
