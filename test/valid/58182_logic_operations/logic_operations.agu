let main(_) : Unit -> Unit =
    let a : Bool = true ;
    let b : Bool = false ;
    let c : Bool = true ;

    let operations : Bool[] = new Bool[5 | false] ;
    set operations[0] = a && (b || c) ;
    set operations[1] = (a && b) || c ;
    set operations[2] = !(a && b) || (b && c) ;
    set operations[3] = !a && b ;
    set operations[4] = !((a || b) && c) ;

    let i : Int = 0 ;
    while i < length(operations) do (
        print(operations[i]) ; print("\n") ;
        set i = i + 1
    )