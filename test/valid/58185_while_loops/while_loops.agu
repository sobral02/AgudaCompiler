-- Test of nested while loops

let printPattern (n) : Int -> Unit =
    let i : Int = 0 ;
    while i < n do (
        let j : Int = 0 ;
        while j <= i do (
           print(5) ;
            set j = j + 1
        ) ;
        print(0) ;
        set i = i + 1
    )

let main (_) : Unit -> Unit = printPattern(5)